<suite name="crossbrowser" thread-count="2" parallel="tests">
<listeners>
<listener class-name="listeners.ExtentReporter"/>
</listeners>
<test name="running in chrome">
<parameter name="browser" value="chrome"> </parameter>
<classes>
<class name="testScripts.AdminUserTest"> </class>
</classes>
</test>
<test name="running in edge">
<parameter name="browser" value="edge"> </parameter>
<classes>
<class name="testScripts.AdminUserTest"> </class>
</classes>
</test>
<test name="running in firefox">
<parameter name="browser" value="firefox"> </parameter>
<classes>
<class name="testScripts.AdminUserTest"> </class>
</classes>
</test>
</suite>